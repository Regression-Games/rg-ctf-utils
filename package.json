{
  "name": "rg-ctf-utils",
  "version": "1.0.4",
  "description": "Utilities for the Regression Games Capture the Flag game mode",
  "main": "build/index.js",
  "types": "src/",
  "repository": {
    "type": "git",
    "url": "https://github.com/Regression-Games/rg-ctf-utils.git"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "prepublishOnly": "tsc",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "docs": "jsdoc2md src/*.ts --configure jsdoc.json --plugin dmd-readable --template README.hbs > README.md",
    "prepare": "husky install"
  },
  "author": "Regression Games, Inc",
  "license": "ISC",
  "dependencies": {
    "minecraft-data": "3.17.0",
    "mineflayer": "4.5.1",
    "mineflayer-pathfinder": "2.4.0",
    "prismarine-entity": "2.1.1",
    "prismarine-item": "1.11.5",
    "rg-bot": "1.9.0",
    "rg-match-info": "1.1.0",
    "vec3": "0.1.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "dmd-readable": "^1.2.4",
    "eslint": "^8.34.0",
    "jsdoc-babel": "^0.5.0",
    "jsdoc-to-markdown": "^8.0.0",
    "prettier": "^2.8.4",
    "typescript": "^4.9.5",
    "husky": "^8.0.0"
  },
  "files": [
    "build/",
    "src/"
  ]
}
